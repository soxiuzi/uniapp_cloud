@font-face {
  font-family: 'iconfont';  /* project id 1487619 */
  src: url('https://at.alicdn.com/t/font_1487619_gtosntqw91w.eot');
  src: url('https://at.alicdn.com/t/font_1487619_gtosntqw91w.eot?#iefix') format('embedded-opentype'),
  url('https://at.alicdn.com/t/font_1487619_gtosntqw91w.woff2') format('woff2'),
  url('https://at.alicdn.com/t/font_1487619_gtosntqw91w.woff') format('woff'),
  url('https://at.alicdn.com/t/font_1487619_gtosntqw91w.ttf') format('truetype'),
  url('https://at.alicdn.com/t/font_1487619_gtosntqw91w.svg#iconfont') format('svg');
}
.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.iconwodedingdan:before {
  content: "\e64f";
}
.iconcailing:before {
  content: "\e604";
}
page {
  font-size: 28rpx;
  line-height: 1.5;
  color: #2c3e50;
  background: #fff;
  -webkit-overflow-scrolling: touch;
  /*使iOS列表滑动流畅*/
}
button {
  color: #333;
}
page,
view,
input,
text,
form,
navigator,
rich-text,
picker,
scroll-view,
cover-view,
open-data {
  box-sizing: border-box;
}
rich-text,
open-data,
form {
  display: block;
}
cover-view {
  line-height: 1.5;
  white-space: normal;
}
/* float */
.fl {
  float: left;
}
.fr {
  float: right;
}
.dpn {
  display: none;
}
.clearfix:after {
  content: '.';
  display: block;
  height: 0;
  clear: both;
  visibility: hidden;
}
.cf6 {
  color: #c7a769;
}
.bgf {
  background: #fff;
}
.rel {
  position: relative;
}
.tac {
  text-align: center;
}
/* 隐藏滚动条 */
::-webkit-scrollbar {
  display: none;
}
button:after {
  border: 0;
}
.flex-box {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.flex-box .flex-item {
    -webkit-box-flex: 1;
    -webkit-flex: 1;
            flex: 1;
}
.footer-box {
  height: 100rpx;
}
/* // 单双行换行 */
.ellipsis {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.ellipsis2 {
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.line-item {
  background: #fff;
  padding-left: 30rpx;
  color: #666;
  line-height: 100rpx;
  font-size: 27rpx;
}
.line-item.arrow {
    position: relative;
}
.line-item.arrow:before {
      position: absolute;
      content: '';
      right: 40rpx;
      top: 50rpx;
      width: 15rpx;
      height: 15rpx;
      border-top: 3rpx solid #ccc;
      border-right: 3rpx solid #ccc;
      -webkit-transform: rotate(45deg);
              transform: rotate(45deg);
}
.line-item .lab {
    float: left;
    line-height: 100rpx;
    height: 100rpx;
    color: #000;
}

